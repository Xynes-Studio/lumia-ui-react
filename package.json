    "npm": "^10.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^6.1.8",
    "uuid": "^9.0.1",
    "vite-plugin-dts": "^3.9.1"
  },
  "devDependencies": {
